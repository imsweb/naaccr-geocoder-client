<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Class name="~.*\.*Test.*"/>
    </Match>
    <Match>
        <Bug code="EI,EI2"/>
    </Match>
    <Match>
         <!-- Do not reuse public identifiers from JSL as class name, fixing this is too disruptive -->
        <Bug pattern="PI_DO_NOT_REUSE_PUBLIC_IDENTIFIERS_CLASS_NAMES"/>
    </Match>
</FindBugsFilter>